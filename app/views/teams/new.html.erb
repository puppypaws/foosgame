<%= form_with scope: :team, url: teams_path, local: true do |form| %>

	<%= form.collection_check_boxes :player_ids, @players, :id, :firstname, :lastname %>

    <p>
        <%= form.label :name %>
        <br>
        <%= form.text_field :name %>
    </p>

    <p>
        <%= form.submit %>
    </p>

<% end %>

